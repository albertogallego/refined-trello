{
  "name": "Refined Trello",
  "version": "0.0.0",
  "description": "Simplifies the Trello interface",
  "homepage_url": "https://github.com/albertogallego/refined-trello",
  "manifest_version": 2,
  "minimum_chrome_version": "60",
  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": ["content.js"],
      "matches": ["https://trello.com/*"]
    }
  ]
}
